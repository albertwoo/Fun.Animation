<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.1-beta</Version>
    <Company>https://www.slaveoftime.fun</Company>
    <Description>React-Spring bindging for fable</Description>
    <PackageProjectUrl>https://github.com/albertwoo/Fun.ReactSpring</PackageProjectUrl>
    <RepositoryUrl>https://github.com/albertwoo/Fun.ReactSpring</RepositoryUrl>
    <PackageTags>Fable, React-Spring</PackageTags>
    <Authors>slaveoftime</Authors>
    <NpmDependencies>
      <NpmPackage Name="react-spring" Version="&gt;= 9.0.0-beta.34" />
    </NpmDependencies>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Hooks.fs" />
    <Compile Include="Animated.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Fable.Core" Version="3.1.4" />
    <PackageReference Include="Fable.React" Version="5.3.5" />
  </ItemGroup>
  
  <ItemGroup>
    <Content Include="*.fsproj; **\*.fs" PackagePath="fable\" />
  </ItemGroup>
</Project>
